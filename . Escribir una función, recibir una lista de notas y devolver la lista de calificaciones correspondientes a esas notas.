def notas_a_calificaciones(notas):
    calificaciones = []
    for nota in notas:
        if nota >= 90:
            calificaciones.append('A')
        elif nota >= 80:
            calificaciones.append('B')
        elif nota >= 70:
            calificaciones.append('C')
        elif nota >= 60:
            calificaciones.append('D')
        else:
            calificaciones.append('F')
    return calificaciones
